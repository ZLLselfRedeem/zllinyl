<%@ Page Language="C#" AutoEventWireup="true" CodeFile="dishManageMutil.aspx.cs" Inherits="CompanyPages_dishManageMutil" %>
<!DOCTYPE html>
<title>悠先收银宝平台-菜品批量添加</title>
<body>
<div>
    <div class="contentContainer" id="page">
    <div class="header header60">
        <div class="headerSprite">
            <div class="text">
		        <h2 class="headerTitle dishManageMutil">批量添加</h2>
             </div>
        </div>
	</div>
    <div class="section">
		<div class="mutilAdd addContainer" id="addContainer">
			<ul class="stepBar">
				<li class="step add cur">1.批量添加菜品图片</li>
				<li class="step edit">2.批量编辑图片</li>
				<li class="step success">3.完成</li>
			</ul>
			<div class="upload" id="upload"><div id="mutilUpload">选择菜品图片</div></div>
			<div class="mutilEdit" id="mutilEdit" style="display:none;">
				<div class="disPicture gray">
					<i class="delete"></i>
                                        <div class="pre-upload-loading"><img width="32" height="32" src="images/loading_2.gif" /></div>
					<ul class="uploadList" id="uploadList"></ul>
					<div id="disSubmit"><a class="btn mutil-next" href="javascript:;">下一张</a></div>
				</div>
				<div id="mutilInfo">
				</div>
			</div>
			<div class="mutilSuccess" id="mutilSuccess" style="display:none;">
				<h3 class="txt">您已成功上传<span id="successPicList">*</span>个菜品</h3>
				<p class="btnSprite">
					<a class="btn continue" href="javascript:;">继续添加</a>
					<a class="btn back" href="javascript:;">返回列表</a>
				</p>
			</div>
		</div>
    </div>

	</div>
</div>
</body>
</html>
