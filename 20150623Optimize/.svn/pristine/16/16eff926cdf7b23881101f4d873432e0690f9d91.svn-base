using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace VAGastronomistMobileApp.Model
{
    /// <summary>
    /// 公司基本信息
    /// </summary>
    public class CompanyInfo
    {
        /// <summary>
        /// 公司编号
        /// </summary>
        public int companyID { get; set; }
        /// <summary>
        /// 公司名称
        /// </summary>
        public string companyName { get; set; }
        /// <summary>
        /// 公司地址
        /// </summary>
        public string companyAddress { get; set; }
        /// <summary>
        /// 公司电话
        /// </summary>
        public string companyTelePhone { get; set; }
        /// <summary>
        /// 公司Logo
        /// </summary>
        public string companyLogo { get; set; }
        /// <summary>
        /// 公司联系人
        /// </summary>
        public string contactPerson { get; set; }
        /// <summary>
        /// 公司联系人电话
        /// </summary>
        public string contactPhone { get; set; }
        /// <summary>
        /// 公司状态
        /// </summary>
        public int companyStatus { get; set; }
        /// <summary>
        /// 图片文件夹路径
        /// </summary>
        public string companyImagePath { get; set; }
        /// <summary>
        /// 公司描述
        /// </summary>
        public string companyDescription { get; set; }
        /// <summary>
        /// 默认菜谱
        /// </summary>
        public string defaultMenuId { get; set; }
        /// <summary>
        /// 预支付的免费退款时间（小时）
        /// </summary>
        public double freeRefundHour { get; set; }
        /// <summary>
        /// 商户支付给友络的佣金的类型
        /// </summary>
        public VACommissionType viewallocCommissionType { get; set; }
        /// <summary>
        /// 商户支付给友络的佣金的值
        /// </summary>
        public double viewallocCommissionValue { get; set; }
        /// <summary>
        /// 公司新浪官方微博
        /// </summary>
        public string sinaWeiboName { get; set; }
        /// <summary>
        /// 人均消费
        /// </summary>
        public double acpp { get; set; }
        /// <summary>
        /// 公司腾讯官方微博
        /// </summary>
        public string qqWeiboName { get; set; }
        /// <summary>
        /// 公司腾讯官方微博
        /// </summary>
        public string wechatPublicName { get; set; }


        /// <summary>
        /// 所属公司
        /// </summary>
        public string ownedCompany { get; set; }
    }

    public class CompanyViewModel
    {
        public int companyID { get; set; }
        public string companyName { get; set; }
    }
}
