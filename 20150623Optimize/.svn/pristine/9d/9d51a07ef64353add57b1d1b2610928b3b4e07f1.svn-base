*{
    margin: 0;
    padding: 0;
    -webkit-tap-highlight-color: rega(0, 0, 0, 0);
}
html,
body{
    width: 100%;
    height: 100%;
    font-size: 13px;
    font-family:Microsoft YaHei,Tahoma,simsun;
    overflow: hidden;
}
.c{
    clear: both;
    width: 0;
    height: 0;
    font-size: 0;
}
.hide{
    display: none;
}
.wrap{
    position: relative;
    #loading{
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        z-index: 8;
        
        img{
            position: absolute;
            top: 50%;
            left: 50%;
            width: 50px;
            height: 50px;
            margin: -25px 0 0 -25px;
            display: block;
        }
    }
    #tipsBg{
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        z-index: 9;
    }
    #tips{
        position: absolute;
        top: 70px;
        left: 10%;
        width: 80%;
        padding: 15px 0;
        text-align: center;
        background: rgba(0,0,0,.8);
        color: #fff;
        border-radius: 10px;
        opacity: 0;
        z-index: 10;
    }
    .main{
        .page{
            .pageContent{
                width: 100%;
                height: 100%;
            }
            .btn{
                width: 80%;
                height: 48px;
                line-height: 48px;
                text-align: center;
                border: 1px solid #aaa;
                border-radius: 10px;
                margin: 10px 10%;
            }
        }
    }
}
.award{
    position: relative;
    width: 100%;
    height: 100%;
    text-align: center;
    
    .awardtip{
        padding-top: 25%;
    }
    .awardInf{
        padding-top: 1%;
        font-size: 1.2em;
        font-weight: bold;
    }
    .awardinstr{
        position: absolute;
        bottom: 7%;
        left: 0;
        width: 100%;
        color: #ffbf9e;
        font-size: 0.9em;
    }
}
.redEnvelope{
    #redEnvelope.hide{
        display: block;
    }
    #redEnvelope{
        background-size: 100% 100%;
        .awardtip{
            color: #ffa73a;
            font-size: 1.25em;
        }
        .awardInf{
            color: #ffc57d;
            font-size: 2em;
        }
        .redEnvelopePrice{
            font-size: 1.6em;
            color: #fed8a8;
            padding-top: 13%;
            span{
                font-size: 1.8em;
            }
        }
        .btn{
            color: #e9b06b;
            font-size: 0.85em;
            border: none;
            margin: 0 10%;
            bottom: 3%;
        }
    }
}
.freeQueue{
        background-size: 100% 100%;
    #freeQueue.hide{
        display: block;
    }
    #freeQueue{
        .awardtip{
            color: #7f1a09;
            font-size: 1.25em;
        }
        .awardInf{
            color: #ffc600;
            font-size: 1.4em;
        }
        .shopInf{
            position: relative;
            width: 100%;
            background: url(../img/shopInfbg.png);
            background-size: 100% 100%;
            font-size: 2em;
            color: #ffefea;
            span,
            p{
                font-size: 0.75em;
            }
            p{
                display: inline-block;
            }
        }
        .awardinstr{
            color: #ffc0b6;
            font-size: 0.85em;
        }
    }
}
.dishs{
    background: url(../img/dishs.png);
    background-size: 100% 100%;
    #dishs.hide{
        display: block;
    }
    #dishs{
        .awardtip{
            color: #828364;
            font-size: 1.25em;
        }
        .awardInf{
            color: #7e7e35;
            font-size: 2em;
        }
        .dishImg{
            position: absolute;
            top: 50%;
            left: 50%;
            overflow: hidden;
            border-radius: 100%;
            img{
                width: 100%;
                height: 100%;
                border-radius: 100%;
            }
        }
        .awardinstr{
            color: #5c4444;
            bottom: 6.5%;
        }
    }
}
.others{
    background: url(../img/others.png);
    background-size: 100% 100%;
    #others.hide{
        display: block;
    }
    #others{
        .awardtip{
            color: #92610b;
            font-size: 1.25em;
        }
        .awardInf{
            color: #7c4d00;
            font-size: 2em;
        }
        .awardinstr{
            color: #904828;
            bottom: 12%;
            input{
                width: 120px;
                height: 34px;
                line-height: 34px;
                text-align: center;
                border-radius: 5px;
                border: none;
                background: #ffeccc;
                color: #904828;
                font-size: 1.8em;
                font-family: Microsoft YaHei, Tahoma, simsun;
            }
        }
    }
}
