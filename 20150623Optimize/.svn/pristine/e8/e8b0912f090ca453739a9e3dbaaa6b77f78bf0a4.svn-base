* {
  margin: 0;
  padding: 0;
  -webkit-tap-highlight-color: rega(0, 0, 0, 0);
}
html,
body {
  position: relative;
  width: 100%;
  font-size: 12px;
  font-family: Microsoft YaHei, Tahoma, simsun;
  overflow: hidden;
}
.c {
  clear: both;
  width: 0;
  height: 0;
  font-size: 0;
}
.hide {
  display: none;
}
ul li {
  list-style-type: none;
}
ol li {
  list-style-type: decimal;
  margin-left: 20px;
}
.wrap {
  overflow: hidden;
  background: #aaa;
  background: url(../img/bg.png);
  background-size: 100% 100%;
}
.wrap #loading {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 8;
}
.wrap #loading img {
  position: absolute;
  top: 50%;
  left: 50%;
  width: 50px;
  height: 50px;
  margin: -25px 0 0 -25px;
  display: block;
}
.wrap #tipsBg {
  position: absolute;
  top: 0;
  left: 0;
  width: 320px;
  height: 480px;
  z-index: 9;
}
.wrap #tips {
  position: absolute;
  top: 70px;
  left: 10%;
  width: 80%;
  padding: 15px 0;
  text-align: center;
  background: rgba(0, 0, 0, 0.8);
  color: #fff;
  border-radius: 10px;
  opacity: 0;
  z-index: 10;
}
.wrap .main {
  width: 100%;
}
.wrap #mark {
  position: absolute;
  bottom: 5px;
  width: 280px;
  text-align: center;
}
.wrap #mark span {
  margin-right: 5px;
}
.wrap .swiper-pagination-bullet-active {
  background: #666;
}
