<!DOCTYPE html>
<html>
	<head>
		<meta charset="utf-8">
		<title>悠先点菜</title>
		<script type="text/javascript">
		  function GetRequest() {
        var url = location.search;
        var theRequest = new Object();
        if (url.indexOf("?") != -1) {
          var str = url.substr(1);
          var strs = str.split("&");
          for (var i = 0; i < strs.length; i++) {
              theRequest[strs[i].split("=")[0]] = unescape(strs[i].split("=")[1]);
          }
        }
        return theRequest;
      }
		  window.onload = function(){
		    location.href = '';
        var Request = new Object();
        Request = GetRequest();
        var code = Request["c"];//活动映射ID
        switch (code) {
          case "1":
            location.href = "http://www.u-xian.com/SpecialSubject/statics/getCouponHZ.html";
            break;
          case "2":
            location.href = "http://www.u-xian.com/SpecialSubject/statics/getCouponSH.html";
            break;
        }
		  }
		</script>
	</head>
	<body>
	</body>
</html>
