*{ outline:none;}
body,html{
	height: 100%;
	font-family:Microsoft YaHei,Tahoma,simsun;
	background-color:#fff;
	z-index: 1;
}
section{
	width: 100%;
	background-color:#fff;
	position:absolute;
	-webkit-transform:rotateY(-180deg) translateZ(250px);
}
section.active{
	-webkit-transform:rotateY(0deg) translateZ(0px);
}

/*加载*/
.wrap {
	position: relative;
	margin: 0 auto;
	text-align: center;
	background-color:#fff;
	-webkit-transform-style:preserve-3d;
	-webkit-perspective:400px;-webkit-perspective-origin:0% 50%;
}
.wrap .img {
  -webkit-animation: play 600ms 300ms infinite;
  -moz-animation: play 600ms 300ms infinite;
  -o-animation: play 600ms 300ms infinite;
  animation: play 600ms 300ms infinite;
  width: 150px;
  height: 150px;
  margin: 0 auto;
}
@-webkit-keyframes play {
  from {
    background-image: url("http://image.u-xian.com/UploadFiles/Images/TreasureChest/loading1.png");
  }
  to {
    background-image: url("http://image.u-xian.com/UploadFiles/Images/TreasureChest/loading2.png");
  }
}
@-moz-keyframes play {
  from {
    background-image: url("http://image.u-xian.com/UploadFiles/Images/TreasureChest/loading1.png");
  }
  to {
    background-image: url("http://image.u-xian.com/UploadFiles/Images/TreasureChest/loading2.png");
  }
}
@-o-keyframes play {
  from {
    background-image: url("http://image.u-xian.com/UploadFiles/Images/TreasureChest/loading1.png");
  }
  to {
    background-image: url("http://image.u-xian.com/UploadFiles/Images/TreasureChest/loading2.png");
  }
}
@keyframes play {
  from {
    background-image: url("http://image.u-xian.com/UploadFiles/Images/TreasureChest/loading1.png");
  }
  to {
    background-image: url("http://image.u-xian.com/UploadFiles/Images/TreasureChest/loading2.png");
  }
}
.wrap .img + div {
  padding-top: 15px;
  font-size: 28px;
  padding-bottom: 15px;
}

#loading{ z-index:99;}

#stage,#shape{
	width: 100%;
}
#stage {
	-webkit-transition: -webkit-transform 2s;
	-webkit-transform-style: preserve-3d;
}
#shape {
	position: relative;
	-webkit-transform-style: preserve-3d;
}

section {
	position: absolute;
	left:0;
	top:0;
	width: 100%;
	color: black;
	background-color: #fff;
	-webkit-backface-visibility: hidden;
	-webkit-transform:  rotateY(180deg) translateZ(1px);
}

section .content{ height:100%; width:100%; position:relative;}

#shape.backfaces .plane {
	-webkit-backface-visibility: visible;
}

.cube > .active {
	-webkit-transform:translateZ(0px);
}

#cutdown{ background-color:#ed1c24; color:#fff; font-size:14px;}
#cutdown .clock{ height:200px; width:100%; position:absolute; top:50%; margin-top:-100px;}
#cutdown .clock .icon{ margin-bottom:30px;}
#cutdown .clock .icon .fa{ font-size:100px; color: #fae9e9}
#cutdown strong{ font-weight:normal;}

.app-content{ font-size:14px!important; font-weight:normal; color:#333;}
.banner{ position: relative;}
.banner .text{ position: absolute; bottom:20px; left:0; right:0; z-index:99;}
.snow .content{ width:100%; height:100%; position:absolute; top:0px; right:0px; bottom:0px; left:0px; z-index:1;}
.snow .content canvas{ z-index:1;}
.snow .content .snows{ height:100%; z-index: -1;}
.snow .content .snows span{ border-radius:100%; width:5px; height:5px; font-size:0px; position:absolute; top:0px; left:0px; display:block; background-color:#fff;}



a.button-bg{ display:block; position:relative; width:100%; color:#fff;}
a.button-bg span.val{ display:block; position:absolute; top:0px; left:0px;}


#moneyText{
	padding-bottom: 20px;
	font-size: 85px;
	color: #fff;
}
.oldPhoneNumber{color: #fff6a6; font-size: 24px; padding-bottom: 45px;}
.oldPhoneNumber a{color: #fff6a6; text-decoration: underline;}

.ctitle{font-size: 20px;}

.instr{font-size: 16px; color: #fff; padding-top: 10px;}
.instr1{font-size: 16px; color: #fff;}


/*悠先排行榜*/
.red-list-title{
	background: url('http://image.u-xian.com/UploadFiles/Images/TreasureChest/christmas/hr-bg.png') repeat-x 0 center;
	height: 20px;
	margin-top: 20px;
}
.red-list-title span{padding: 0 10px; background: #fff;}

.redList ul{margin-top: 50px; padding: 0;}
.redList ul li{font-size: 20px; color: #ea3d3d; list-style-type: none; border-bottom: 1px dashed #c4b9a6; padding: 10px 0;}
.pNum{margin-right: 10px; color: #777;}
.stext{color: #b2b2b2;}
.redList .rSquare{width: 76px; height: 76px; border-radius: 9px; margin-left: 5px; color: #fff; font-size: 70px; line-height: 70px;}
.rSquare1{background: #f7860d;}
.rSquare2{background: #ff9b2f;}
.rSquare3{background: #ffbd4a;}
.rSquare4{background: #ffc86a;}
.rSquare5{background: #ffd791;}

/*音乐*/
#music_div{
	position: absolute;
	top: 10px;
	right: 10px;
	z-index: 100;
	width: 38px;
	height: 39px;
	background: url('img/music-off.png') no-repeat;
}
#music_div.stop{
	background: url('img/music-on.png') no-repeat;
}


/*号码纠错*/
.va-row{padding-top: 30px;padding-bottom: 30px;}
.va-row input{ height:50px;}
.va-modal-body {
padding-bottom: 0;
padding-top: 0;
}
.button-footer button {
border-right-width: 0;
border-bottom-width: 0;
padding: 0;
height: 45px;
border-radius: 0;
border-bottom-right-radius: 6px;
}
.button-footer .left-button {
border-bottom-left-radius: 6px;
border-bottom-right-radius: 0;
border-left-width: 0;
}


.obtn{ height:70px; border-radius:9px; border-bottom: 4px solid #5ba525; background: #63c81a; color: #fff; line-height: 66px; padding:0; font-size:20px;}
.oipt{ height:70px; border-radius: 9px; font-size:20px;}

.text .col-xs-8,
.text .col-xs-4{ padding-left: 15px; padding-right:15px;}

.obtn:hover,
.obtn:active{ color: #fff;}

@media (max-width:480px){
	.obtn{ height:50px; border-radius:7px; border-bottom: 3px solid #5ba525; line-height: 47px; font-size:16px;}
	.oipt{ height:50px; border-radius: 7px; font-size:16px;}
	.text .col-xs-8,
	.text .col-xs-4{ padding-left: 10px; padding-right:10px;}
	#moneyText{ font-size: 65px;}
	.oldPhoneNumber{font-size: 20px; padding-bottom: 35px;}
	.instr{padding-top: 10px;}
	.instr, .instr1{font-size: 13px;}
	.redList .rSquare{width: 57px; height: 57px; font-size: 50px; line-height: 57px;}
	.redList ul li{font-size: 16px!important;padding: 7px 0;}
	#moneyText{padding-bottom: 10px;}
	.ctitle{font-size: 20px;}
	.snow .text{bottom: 15px;}
	.banner .text{bottom: 13px;}
}
@media (max-width:400px){
	.oldPhoneNumber, .ctitle{font-size: 14px;}
	#moneyText span{padding-left:30px;}
	.oldPhoneNumber{font-size: 20px; padding-bottom: 10px;}
	a.button-bg span.val{ font-size:20px;}
	.text .col-xs-8 input{ font-size:20px;}
}
@media (max-width:360px){
	.obtn{ height:35px; border-radius:5px;border-bottom: 2px solid #5ba525; line-height: 33px; font-size:12px;}
	.oipt{ height:35px; border-radius: 5px; font-size:12px;}
	.text .col-xs-8,
	.text .col-xs-4{ padding-left: 5px; padding-right:5px;}
	#moneyText{ font-size: 45px;}
	.oldPhoneNumber{font-size: 16px; padding-bottom: 25px;}
	.instr{padding-top: 5px;}
	.instr, .instr1{font-size: 12px;}
	.redList .rSquare{width: 38px; height: 38px; font-size: 30px; line-height: 38px; font-weight: bold;}
	.redList ul li{font-size: 13px!important;  padding: 5px 0;}
	#moneyText{padding-bottom: 10px;}
	.ctitle{font-size: 14px;}
	.banner .text{bottom: 8px;}
}
@media (max-width:320px){
	.obtn{ height:35px; border-radius:5px;border-bottom: 2px solid #5ba525; line-height: 33px; font-size:12px;}
	.oipt{ height:35px; border-radius: 5px; font-size:12px;}
	.text .col-xs-8,
	.text .col-xs-4{ padding-left: 5px; padding-right:5px;}
	#moneyText{ font-size: 45px;}
	.oldPhoneNumber{font-size: 16px; padding-bottom:20px;}
	.instr{padding-top: 5px;}
	.instr, .instr1{font-size: 12px;}
	.redList .rSquare{width: 38px; height: 38px; font-size: 30px; line-height: 38px; font-weight: bold;}
	.redList ul li{font-size: 12px!important; padding: 5px 0;}
	#moneyText{padding-bottom: 0;}
	.ctitle{font-size: 12px;}
	.snow .text{bottom: 10px;}
	.banner .text{bottom: 5px;}
}




