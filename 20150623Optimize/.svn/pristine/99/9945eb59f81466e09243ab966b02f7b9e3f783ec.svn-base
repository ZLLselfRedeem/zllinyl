CREATE TABLE [dbo].[CompanyAccounts] (
    [identity_Id] INT            IDENTITY (1, 1) NOT NULL,
    [companyId]   INT            NOT NULL,
    [accountNum]  VARCHAR (40)   NOT NULL,
    [bankName]    NVARCHAR (100) NULL,
    [remark]      NVARCHAR (200) NULL,
    [accountName] NVARCHAR (50)  NULL,
    [status]      INT            NULL,
    CONSTRAINT [PK_CompanyAccounts] PRIMARY KEY CLUSTERED ([identity_Id] ASC)
);

