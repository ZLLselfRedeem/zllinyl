/*遮照*/
.mask {
  display: none;
  max-width: 640px;
  width: 100%;
  height: 100%;
  background: #000000;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  z-index: 9;
  margin-left: auto;
  margin-right: auto;
}
.mask div {
  color: white;
  font-size: 162.5%;
}
.mask img {
  width: 100%;
  margin: 0 auto;
}
.mask .img-play {
  -webkit-animation: maskPlay 600ms 300ms 2;
  -moz-animation: maskPlay 600ms 300ms 2;
  -o-animation: maskPlay 600ms 300ms 2;
  animation: maskPlay 600ms 300ms 2;
}
.mask div :first-child {
  width: 80%;
}
.mask button {
  width: 93.75%;
  height: 44px;
  font-size: 112.5%;
  line-height: 30px;
  margin-bottom: 0;
  margin: 30px 3.125% 0;
  border-color: #ed1c24;
}
.mask button,
.mask button:hover,
.mask button:focus,
.mask button:active {
  background: #ed1c24;
  color: #ffffff;
  border-color: #ed1c24;
}
.mask .button-wr,
.mask .button-wr:hover,
.mask .button-wr:focus,
.mask .button-wr:active {
  background: #ffffff;
  color: #ed1c24;
  border-color: #ffffff;
}
.mask .button-hui,
.mask .button-hui:hover,
.mask .button-hui:focus,
.mask .button-hui:active {
  background: #c2c2c2;
  color: #ffffff;
  border-color: #c2c2c2;
}
@-webkit-keyframes maskPlay {
  from {
    background-image: url("http://image.u-xian.com/UploadFiles/Images/TreasureChest//shard_a.png");
    background-repeat: no-repeat;
    background-size: auto 100%;
  }
  to {
    background-image: url("http://image.u-xian.com/UploadFiles/Images/TreasureChest/shard_b.png");
    background-repeat: no-repeat;
    background-size: auto 100%;
  }
}
@-moz-keyframes maskPlay {
  from {
    background-image: url("http://image.u-xian.com/UploadFiles/Images/TreasureChest/shard_a.png");
    background-repeat: no-repeat;
    background-size: auto 100%;
  }
  to {
    background-image: url("http://image.u-xian.com/UploadFiles/Images/TreasureChest/shard_b.png");
    background-repeat: no-repeat;
    background-size: auto 100%;
  }
}
@-o-keyframes maskPlay {
  from {
    background-image: url("http://image.u-xian.com/UploadFiles/Images/TreasureChest/shard_a.png");
    background-repeat: no-repeat;
    background-size: auto 100%;
  }
  to {
    background-image: url("http://image.u-xian.com/UploadFiles/Images/TreasureChest/shard_b.png");
    background-repeat: no-repeat;
    background-size: auto 100%;
  }
}
@keyframes maskPlay {
  from {
    background-image: url("http://image.u-xian.com/UploadFiles/Images/TreasureChest/shard_a.png");
    background-repeat: no-repeat;
    background-size: auto 100%;
  }
  to {
    background-image: url("http://image.u-xian.com/UploadFiles/Images/TreasureChest/shard_b.png");
