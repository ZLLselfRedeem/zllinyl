/*加载*/
.loading {
  max-width: 640px;
  height: 100%;
  width: 100%;
  font-size: 187.5%;
  text-align: center;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  color: #958d7b;
  padding-top: 60px;
  margin: 0 auto;
}
.loading .img {
  -webkit-animation: play 600ms 300ms infinite;
  -moz-animation: play 600ms 300ms infinite;
  -o-animation: play 600ms 300ms infinite;
  animation: play 600ms 300ms infinite;
  width: 150px;
  height: 150px;
  margin: 0 auto;
}
@-webkit-keyframes play {
  from {
    background-image: url("http://image.u-xian.com/UploadFiles/Images/TreasureChest/loading1.png");
  }
  to {
    background-image: url("http://image.u-xian.com/UploadFiles/Images/TreasureChest/loading2.png");
  }
}
@-moz-keyframes play {
  from {
    background-image: url("http://image.u-xian.com/UploadFiles/Images/TreasureChest/loading1.png");
  }
  to {
    background-image: url("http://image.u-xian.com/UploadFiles/Images/TreasureChest/loading2.png");
  }
}
@-o-keyframes play {
  from {
    background-image: url("http://image.u-xian.com/UploadFiles/Images/TreasureChest/loading1.png");
  }
  to {
    background-image: url("http://image.u-xian.com/UploadFiles/Images/TreasureChest/loading2.png");
  }
}
@keyframes play {
  from {
    background-image: url("http://image.u-xian.com/UploadFiles/Images/TreasureChest/loading1.png");
  }
  to {
    background-image: url("http://image.u-xian.com/UploadFiles/Images/TreasureChest/loading2.png");
  }
}
.loading .img + div {
  padding-top: 15px;
  font-size: 28px;
  padding-bottom: 15px;
}
.loading div + div {
  font-size: 20px;
}
