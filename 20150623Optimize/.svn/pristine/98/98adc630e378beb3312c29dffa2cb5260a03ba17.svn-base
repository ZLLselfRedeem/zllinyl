html{
    overflow-y: auto;
	font-size:75%; /* 1rem = 12px */
}
body{
    margin: 0px;
	/*font-size:62.5%; 16px*62.5%=10px */
}
body, ul, li, div, h1, h2{
    padding: 0;
    margin: 0;
    font-size: 15px;
    color: #3b3c3c;
}

td{
    font-family: Arial, Helvetica, sans-serif;
}
div{
    font-size: 15px;
    font-family: Verdana, Arial, Helvetica, sans-serif;
}

li{
    list-style: none;
}
p{
    font-size: 12px;
    font-family: Verdana, Arial, Helvetica, sans-serif;
}

img{
    border: 0;
}

table{
    line-height: normal;
}


A:link{
    text-decoration: none;
}
A:visited{
    text-decoration: none;
    color: #000000;
}
A:active{
    text-decoration: none;
}
A:hover{
    text-decoration: none;
}
