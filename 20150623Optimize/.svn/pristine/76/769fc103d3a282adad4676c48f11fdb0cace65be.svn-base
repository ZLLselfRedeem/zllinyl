}
.dishMix .tip .txt{
	color:#ff0000;
	font-weight:900;
	font-style:normal;
}
.dishMix .dataContainer #dataTable tr td .inputCont .tip{
	display:none;
	position:absolute;
	width:205px;
	height:40px;
	line-height:180%;
