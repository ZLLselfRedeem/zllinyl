/* 弹出层  */
.tipsup,
.popup{ 
	background:url(../images/bg_popup2.png) left top no-repeat;
	_background:url(../images/bg_popup.png) left top no-repeat;
	width:255px;
	height:56px;
	padding-top:12px;
	overflow:hidden;
	text-align:center;
}
.tipsup .txt{ font-size:14px;color:#986e4f; }
.popup { 
	padding-top:8px;
	position:absolute;
	left:0px;
	top:-25px;
	height:0; 
}
.popup .txt{ font-size:12px; color:#986e4f;}
.popup .txt strong{ font-weight:900;color:#ff0000; }
.txtColor{
    color:#ff0000;
    font-style:normal;
}
.txtGreen{
    color:#2e8006;
    font-style:normal;
}
#popupSprite{ 
	position:relative;
	left:680px;
}
#uploadOver{ 
	z-index:10;
}
.popupLayout{
	background-color:#ffffff;
	z-index:100;
}

.confirm  .popupSprite .popup{ 
	left:-50px;
}
	/* 
	 * 弹出层结构
	 * 【模拟系统提示、退款操作面板、打印列表选择面板】
	 * 
	 * 
	 */
.popupLayout{ 
	border:1px solid #c6c6c6;
	text-align:center;
	position:relative;
	z-index:30;
}
.popupLayout .text{ 
	width:177px;
	height:140px;
	margin:33px auto;
}
.popupLayout .text .inputText{ 
	width:162px;
	height:30px;
	line-height:30px;
	border:1px solid #c6c6c6;
	background-color:#ffffff;
	padding:0 5px;
	font-size:12px;
	margin-top:22px;
}
.popupLayout .text .btnSprite{ 
	width:172px;
	margin-top:22px;
	position:relative;
}
.popupLayout .text  .btn,
.popupLayout .text  .btn:visited{ 
	width:65px;
	height:25px;
	line-height:25px;
	overflow:hidden;
	display:block;
	float:left;
	background:url(../images/btn_sprite.png) left top no-repeat;
	color:#ffffff;
	font-family:"Microsoft YaHei",arial;
	text-decoration:none;
}
.popupLayout .text  .cancel,
.popupLayout .text  .cancel:visited{ 
	position:absolute;
	right:-3px;
	background-position:-175px 0;
	color:#c6c6c6;
}
.contentContainer .uploadLayout{
	/* width:808px; */
	height:328px;
	padding:10px;
	border:6px solid #ffb400;
	background-color:#ffffff;
	z-index:4;
	overflow:hidden;
	-webkit-border-radius:6px;
	-moz-border-radius:6px;
	border-radius:6px;
	position:relative;
}

	/* 退款操作面板 refundLayout */
	
.refundLayout{
	background-color:#ffffff;
	z-index:4;
	overflow:hidden;
	padding:23px 0px;
	border:1px solid #626262;
}
.refundLayout .headerItem{
	height:45px;
	overflow:hidden;
	line-height:45px;
	color:#3f3f3f;
	font-size:18px;
	font-family:"Microsoft YaHei",arial;
	text-align:center;
}
.refundLayout .comment{
	height:25px;
	line-height:25px;
	font-size:12px;
}
.refundLayout .comment .num{
	color:#ff0000;
	padding:0 8px;
}

.refundLayout  .text{
	font-size:14px;
}
.refundLayout  .text{
	width:300px;
	margin:0 auto;
}
.refundLayout  .text .tips{
	clear:both;
	height:40px;
	line-height:40px;
	font-size:12px;
	padding-left:70px;
}
.refundLayout  .text .tips .account{
	color:#ff0000;
}
.refundLayout  .text label{
	display:block;
	float:left;
	width:70px;
	text-align:right;
}
.refundLayout  .text .refundMoney{
	height:33px;
	line-height:33px;
}
.refundLayout  .text .refundMoney .inputText,
.refundLayout  .text  .remark .area{
	height:33px;
	width:204px;
	line-height:33px;
	padding:0 3px;
	background-color:#ffffff;
	border:1px solid #e3eaf0;
	margin-right:8px;
	font-size:12px;
	resize:none;
}
.refundLayout  .text  .remark{
	margin-top:12px;
}
.refundLayout  .text  .remark,
.refundLayout  .text  .remark  .area{
	height:72px;
}
.refundLayout .btnSprite{
	margin-top:12px;
	padding-left:70px;
	position:relative;
	top:0;
}
.refundLayout .btnSprite a{
	width:90px;
	height:27px;
	line-height:27px;
	float:left;
	display:block;
	text-align:center;
	text-decoration:none;
	background:url('../images/btn_sprite.png') left top no-repeat;
	margin-right:8px;
}

.refundLayout .btnSprite a.comfirm{
	background-position:-75px -174px;
	color:#ffffff;
}
.refundLayout .btnSprite a.cancel{
	background-position:0px -58px;
	color:#7f7f81;
}
	
	/* 打印列表选择面板 printerLayout */
.printerLayout{
	background-color:#ffffff;
	z-index:4;
	overflow:hidden;
	padding:23px 0px;
	border:1px solid #626262;
}
.printerLayout .headerItem{
	height:45px;
	overflow:hidden;
	line-height:45px;
	color:#3f3f3f;
	font-size:18px;
	font-family:"Microsoft YaHei",arial;
	text-align:center;
}
.printerLayout  .text{
	font-size:14px;
	width:300px;
	margin:0 auto;
}
.printerLayout  .text ul li{
	line-height:30px;
	font-size:12px;
}
.printerLayout  .text ul li input{
	position:relative;
	top:3px;
}
.printerLayout .btnSprite{
	margin-top:12px;
	padding-left:70px;
}
.printerLayout .btnSprite a{
	width:90px;
	height:27px;
	line-height:27px;
	float:left;
	display:block;
	text-align:center;
	text-decoration:none;
	background:url('../images/btn_sprite.png') left top no-repeat;
	margin-right:8px;
}

.printerLayout .btnSprite a.comfirm{
	background-position:-75px -174px;
	color:#ffffff;
}
.printerLayout .btnSprite a.cancel{
	background-position:0px -58px;
	color:#7f7f81;
}
	

/* 表单提交验证提示 */
.required{
	position:relative;
	left:0;
	top:0;
