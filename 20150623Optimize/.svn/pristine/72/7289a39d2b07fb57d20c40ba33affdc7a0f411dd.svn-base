<body onload="Transfer()">
  <script>
      function GetRequest() {
          var url = location.search;
          var theRequest = new Object();
          if (url.indexOf("?") != -1) {
              var str = url.substr(1);
              var strs = str.split("&");
              for (var i = 0; i < strs.length; i++) {
                  theRequest[strs[i].split("=")[0]] = unescape(strs[i].split("=")[1]);
              }
          }
          return theRequest;
      }
      function Transfer() {
          location.href = "http://u-xian.com/AppPages/BigRedEnvelope/index.html?activityId=73";
      }
</script>  
