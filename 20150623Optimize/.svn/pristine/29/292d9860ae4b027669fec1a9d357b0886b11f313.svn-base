using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace VAGastronomistMobileApp.Model
{
    /// <summary>
    /// 餐桌信息
    /// </summary>
    public class TableInfo
    {
        /// <summary>
        /// 餐桌编号
        /// </summary>
        public int TableID { get; set; }
        /// <summary>
        /// 餐桌名称
        /// </summary>
        public string TableName { get; set; }
        /// <summary>
        /// 餐桌座位数
        /// </summary>
        public int TableSeats { get; set; }
        /// <summary>
        /// 餐桌状态
        /// -1：已删除，0：编辑中，1：空桌，2：点餐中，3：待确认，4：就餐中，5：已结账
        /// </summary>
        public int TableStatus { get; set; }
        /// <summary>
        /// 餐桌所属区域编号
        /// </summary>
        public int tableAreaID { get; set; }
        /// <summary>
        /// 接口编号
        /// </summary>
        public string interfaceTableno { get; set; }
        /// <summary>
        /// 打印机
        /// </summary>
        public string tablePrinter { get; set; }
    }
}
