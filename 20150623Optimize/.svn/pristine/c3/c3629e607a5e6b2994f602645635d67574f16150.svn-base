body{
    font-size: 13px;
    font-family:Microsoft YaHei,Tahoma,simsun;
}

img{
    display: block;
}
p{
    margin: 0;
}
.c{
    width: 0;
    height: 0;
    font-size: 0;
    clear: both;
}


.loadingImg{
    position: absolute;
    top: 50%;
    width: 15%;
    left: 50%;
    margin: -7.5% 0 0 -7.5%;
}

//券列表
.status0{
    #choose.hide{
        display: block;
    }
}
//领取成功
.status1{
    #got.hide{
        display: block;
    }
}
//活动结束
.status2{
    #gameover.hide{
        display: block;
    }
}


#goGet{
    position: relative;
    /*导航*/
    .checking{
        position: fixed;
        top: 0;
        width: 100%;
        background: rgba(255,255,255,1);
        z-index: 1;
        
        .f_search{
            position: relative;
            width: 100%;
            padding: 8px 0;
            background: #eaeaea;
            
            .f_search_ipt{
                width: 80%;
                height: 30px;
                line-height: 30px;
                margin-left: 10%;
                border-radius: 4px;
                border: none;
                background: #fff url('img/s-bg.png') no-repeat center center;
                background-size: contain;
                -webkit-appearance: none;
            }
            
            .f_search_cur{
                position: absolute;
                top: 0;
                left: 0;
                width: 100%;
                height: 100%;
            }
        }
        
        ul{
            padding: 0;
            margin: 12px auto;
            width: 80%;
            overflow: hidden;
            
            li{
                list-style-type: none;
                width: 33.3%;
                padding: 7px 0;
                text-align: center;
                color: #7b7b7b;
                border: 1px solid #eaeaea;
                background: #fff;
                float: left;
            }
            li:first-child{
                border-radius: 4px 0 0 4px;
                border-right: none;
            }
            li:last-child{
                border-radius: 0 4px 4px 0;
                border-left: none;
            }
            
            .chkCur{
                color: #444;
                background: #eaeaea;
            }
        }
    }
    
    /*抵扣券浏览*/
    .conMain{
        margin-top: 108px;
        width: 100%;
        margin-bottom: 20px;
        
        a{
            display: block;
        }
        
        dl{
            border-top: 1px solid #d0d0d0;
            border-bottom: 1px solid #d0d0d0;
            margin-bottom: 10px;
            margin-left: 4%;
            width: 44%;
            float: left;
            display: block;
            
            p{
                margin: 0;
            }
            
            dd{
                position: relative;
                
                img{
                    width: 100%;
                }
                p{
                    color: #fff;
                    background: #f00;
                    padding: 4px 0;
                    text-align: center;
                }
                
                .endPic{
                    position: absolute;
                    top: 0;
                    left: 0;
                    display: none;
                }
            }
            dt{
                border-left: 1px solid #d0d0d0;
                border-right: 1px solid #d0d0d0;
                padding: 8px 6px;
                
                p{
                    color: #000203;
                }
                .shopADD{
                    font-weight: normal;
                    font-size: 0.9em;
                }
            }
        }
        .shopEndPic{
            dl{
                dd{
                    .endPic{
                        display: block;
                    }
                    p{
                       background: #6e6e6e; 
                    }
                }
            }
        }
    }
    .doload{
        padding: 5% 0;
        img{
            width: 10%;
            margin: 0 auto;
        }
    }
}

/*领取成功*/
#gotten{
    .gottenInfo{
        position: relative;
        
        img{
            width: 100%;
        }
        .info{
            position: absolute;
            bottom: 3%;
            width: 100%;
            
            p{
                color: #616161;
                text-align: center;
                margin: 0;
            }
            
            .des{
                width: 70%;
                color: #fff;
                margin-bottom: 4%;
                margin-left: 15%;
                span{
                    font-size: 1.4em;
                }
            }
            .number{
                color: #fff;
                margin-bottom: 2%;
                
                a,
                a:hover,
                a:active{
                    color: #fff834;
                    text-decoration: underline;
                    padding: 7px;
                }
            }
            .oneHourDes{
                color: #ffffff!important;
                font-size: 0.8em;
            }
            .downbtn{
                margin: 0 auto 2% auto;
            }
        }
    }
    .other{
        width: 100%;
        background: #fffee3;
        overflow: hidden;
        
        .titleImg{
            width: 100%;
            margin: 18px 0;
        }
        dl{
            float: left;
            width: 44%;
            background: #fff;
            padding: 2%;
            margin-bottom: 10px;
            margin-left: 4%;
            -webkit-box-shadow: 0px 2px 5px #bbb;
            
            dd{
                img{
                    width: 100%;
                }
            }
            dt{
                p{
                    text-align: right;
                }
                #dishName{
                    text-align: left;
                    padding: 5px 0;
                }
                #oldPrice{
                    color: #888;
                }
                #nowPrice{
                    font-size: 1.3em;
                    color: #e44934;
                }
            }
        }
    }
}

/*已抢完 已过期*/
#over{
    width: 100%;
    
    img{
        width: 100%;
    }
}

/*透明黑底*/
.mask{
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: #000;
    opacity: 0.8;
    z-index: 99;
}

.popupBox{
    position: fixed;
    top: 50px;
    left: 10%;
    width: 80%;
    border-radius: 0.6em;
    padding: 26px 23px;
    background: #fff;
    z-index: 100;
}

/*领取页输入手机号弹出框*/
.iptInfo{
    p{
        margin-bottom: 10px;
    }
    
    input{
        border: 1px solid #030101;
        border-radius: 0.4em;
        height: 48px;
    }
    
    #phoneNumber{
        width: 100%;
        padding: 0 1em;
    }
    
    .btn{
        width: 48%;
        background: #fff;
    }
    #confirm{
        border-color: #ff1717;
        color: #ff1717;
    }
}

/*提示 号码纠错*/
.msg{
    padding: 0;
    p{
        text-align: center;
        width: 80%;
        padding: 7px 0;
        margin: 0 10%;
        
    }
    .current{
        p:first-child{
            margin-top: 10px;
        }
        
        input{
            width: 100%;
            padding: 1em;
            border-radius: 4px;
            border: 1px solid #666;
        }
        
        ul{
            width: 100%;
            padding: 0;
            margin: 10px 0 0 0;
            border-top: 1px solid #666;
            overflow: hidden;
            
            li{
                width: 50%;
                float: left;
                text-align: center;
                list-style-type: none;
                padding: 15px 0;
            }
            #cencelCur{
                border-right: 1px solid #666;
            }
        }
    }
}

/*搜索*/
#search_content{
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    min-height: 100%;
    background: #f8f8f8;
    z-index: 10;
    
    .s{
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        z-index: 3;
        padding: 8px 0;
        background: #eaeaea;
        
        #search_ipt{
            width: 76%;
            height: 30px;
            line-height: 30px;
            padding: 0 0.5em;
            margin-left: 4%;
            border: none;
            border-radius: 4px;
            background: #fff;
            //background: #fff url('img/s-bg.png') no-repeat center center;
            background-size: contain;
        }
        .search_ipt_focus{
            background: none;
        }
        #search_cancel{
            width: 20%;
            padding: 6px 0;
            text-align: center;
            color: #646464;
            display: inline-block;
        }
        .submitIpt{
            position: absolute;
            top: 0;
            left: 0;
        }
    }
    .search_filter{
        position: absolute;
        top: 50px;
        left: 0;
        z-index: 2;
        width: 96%;
        background: #fff;
        border-radius: 2px;
        border: 1px solid #eee;
        margin-left: 2%;
        padding: 0;
        min-height: 100%;
        
        li{
            list-style-type: none;
            width: 100%;
            height: 36px;
            line-height: 36px;
            border-bottom: 1px solid #eee;
            padding: 0 1em;
        }
    }
    .search_result{
        overflow: hidden;
        background: #fff;
        padding: 50px 0 70px 0;
        
        a,
        a:hover,
        a:active{
            text-decoration: none;
            color: initial;
        }
        
        dl{
            margin: 0;
            border-top: 1px solid #d0d0d0;
            border-bottom: 1px solid #d0d0d0;
            width: 44%;
            margin-top: 7px;
            margin-left: 4%;
            float: left;
            
            dd{
                position: relative;
                img{
                    width: 100%;
                }
                p{
                    color: #fff;
                    background: #f00;
                    padding: 4px 0;
                    text-align: center;
                }
                
                .endPic{
                    position: absolute;
                    top: 0;
                    left: 0;
                    display: none;
                }
            }
            dt{
                border-left: 1px solid #d0d0d0;
                border-right: 1px solid #d0d0d0;
                padding: 8px 6px;
                
                p{
                    overflow: hidden;
                }
                
                .shopname{
                    height: 36px;
                }
                .shopADD{
                    height: 51px;
                    font-weight: normal;
                    font-size: 0.9em;
                }
            }
        }
        .shopEndPic{
            dl{
                dd{
                    .endPic{
                        display: block;
                    }
                    p{
                       background: #6e6e6e; 
                    }
                }
            }
        }
    }
}

