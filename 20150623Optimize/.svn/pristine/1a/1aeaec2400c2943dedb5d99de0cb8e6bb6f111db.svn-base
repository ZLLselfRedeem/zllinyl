using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace VAGastronomistMobileApp.Model
{
    /// <summary>
    /// 用户邀请记录基本信息
    /// </summary>
    public class CustomerInviteRecord
    {
        /// <summary>
        /// 编号
        /// </summary>
        public long id { get; set; }
        /// <summary>
        /// 邀请用户编号
        /// </summary>
        public long customerId { get; set; }
        /// <summary>
        /// 邀请用户的手机号码
        /// </summary>
        public string phoneNumberInvite { get; set; }
        /// <summary>
        /// 被邀请用户的手机号码
        /// </summary>
        public string phoneNumberInvited { get; set; }
        /// <summary>
        /// 邀请时间
        /// </summary>
        public DateTime inviteTime { get; set; }
        /// <summary>
        /// 该邀请获得奖励时间
        /// </summary>
        public DateTime inviteRewardTime { get; set; }
        /// <summary>
        /// 该邀请获得奖励时间
        /// </summary>
        public double inviteRewardValue { get; set; }
    }
}
