
#diaries{
    text-align:center;
    overflow:hidden;
    font-family:"Microsoft Yahei",arial;
}

#pageLoading{
    text-align:center;
    padding-top:50%;
    background:#ffffff;
}
.pageLoading .txt{
    font-size:1.3rem;
}
.pageLoading .comment{
    font-size:1rem;
}

.food-diaries .header-top{
    width:96%;
    margin:0 auto;
    text-align:right;
    height:35px;
    line-height:35px;
    position:relative;
}
.food-diaries .header-top h2{
    font-weight:normal;
    height:40px;
    line-height:40px;
    overflow:hidden;
}

.food-diaries .header-top .date{
    float:left;
}
.food-diaries .header-top i{
    font-style:normal;
}
.food-diaries .comment{
    border-top:1px solid #c7cdd3;
    border-bottom:1px solid #c7cdd3;
    margin-bottom:12px;
    background-color:#ffffff;
}
.food-diaries .comment .area{
    border-radius:5px;
    min-height:90px;
    width:96%;
    margin:0px auto;
    border-color:#c6ccd2;
    font-size:1rem;
    font-family:"Microsoft Yahei",arial;
    border-width:0;
    box-shadow: 0 0px 0px rgba(0, 0, 0, 0.2) inset;
    /* resize:vertical; */
    background: #fff;
    overflow: hidden;
    vertical-align: top; 
    transition: height 0.2s;
    -webkit-transition: height 0.2s; 
    -moz-transition: height 0.2s; 

}
.food-diaries .comment .txtColor{
    color:#9a9b9b; 
}
.food-diaries .comment .txt-default-color{
    color:#cbcbcb;
}
.food-diaries .comment .btnSprite{
    width:96%;
    margin:0 auto;
    padding-bottom:12px;
    height:30px;
}
.food-diaries .comment .btnSprite a{
    float:right;
    width:70px;
    height:24px;
    margin-left:5px;
    overflow:hidden;
    background-position:0 0;
    background-repeat:no-repeat;
    background-size:contain;
    text-indent:-1000px;
}
.food-diaries .comment .btnSprite a.switch-txt{
    background-image:url("../img/txt_1.png");
}
.food-diaries .comment .btnSprite a.clear-txt{
    background-image:url("../img/txt_2.png");
}

#diariesShow,
#diariesWebShow{
    text-align:center;
    width:100%;
    /* margin:0 auto; */
    font-family:"Microsoft Yahei",arial;
    background-color:#f9f9f9;
}
.food-diaries-show .header-top{
    background:url('../img/share_bg_header.png') left top no-repeat;
    background-size:contain;
    width:100%;
    position:relative;
}
.food-diaries-show .header-top h2 .txt{
    width:96%;
    height:18px;
    text-align:right;
    position:absolute;
    left:2%;
    bottom:5px;
    overflow:hidden;
    text-shadow: 0 0px 0 #F3F3F3;
    color:#000;
    font-size:0.8rem;
}
.food-diaries-show .header-top .date{
    float:left;
}
.food-diaries-show .header-top i{
    font-style:normal;
}
.food-diaries-show .pics-list{
    border-bottom: 1px solid #c7cdd3;
}
.food-diaries-show .diaries-show-content{
    text-align:center;
    border-bottom:1px solid #c7cdd3;
}
.food-diaries-show .diaries-show-content .comment{
    width:96%;
    margin:0 auto;
    text-align:left;
    padding:18px 0;
    font-size:1rem;
    text-indent:25px;
}

.food-diaries-show .diaries-show-content .diariesComment{
    text-align:justify;
    width:96%;
    margin:0 auto;
}
.food-diaries-show .diaries-show-content .diariesComment .title{
    height:35px;
    line-height:35px;
    color:#000000;
    text-align:center;
    border-bottom:1px dotted #000000;
}
.food-diaries-show .diaries-show-content .diariesComment .title h3{
    margin:0;
    position:relative;
    top:18px;
    margin:0 auto;
    width:45%;
    background-color:#f9f9f9;
}
.food-diaries-show .diaries-show-content .diariesComment .text{
    margin-top:25px;
    margin-bottom:25px;
    font-family:"Microsoft Yahei",arial;
}
.food-diaries-show .diaries-show-content .diariesComment .text a{
    color:#3b3c3c;
    font-weight:normal;
}
.food-diaries-show .diaries-show-content .diariesComment .text .txtColor{
    color:#084eff;
}


.web-container{
    width:640px;
    margin:0 auto;
}
.food-diaries-show  .web-container .header-top{
    height:100px;
    background-size:cover;
}

.pics-list{
    padding-top:12px;
    border-top:1px solid #c7cdd3;
}
.pics-list .text{
    width:100%;
    margin:0px auto 12px auto;
    /* overflow-y:scroll; */
}

.pics-list .text:after{
    content:'\20';
    display:block;
    height:0px;
    clear:both;
}

.pics-list .text li{
    float:left;
    width:47%;
    margin-left:2%;
    margin-bottom:2%;
    position:relative;
    border-width:0;
    overflow:hidden;
}
.pics-list .text li a{
    position:relative;
    z-index:1;
}
.pics-list .text li a:after{
    content:'\20';
    height:0;
    display:block;
    clear:both;
}

.pics-list .text li img{
    width:100%;
    float:left;
}
.pics-list .text li .txt{
    position:absolute;
    left:0;
    bottom:6%;
    width:98%;/* margin-right:2%; */
    z-index:4;
    font-weight:normal;
    color:#ffffff;
    text-shadow: 0 0px 0 #F3F3F3;
}
.pics-list .text li .masker{
    position:absolute;
    left:0px;
    top:0px;
    width:100%;
    height:100%;
    z-index:2;
    /*
    background:url('../img/drag_masker.png') left top no-repeat;
    background-size:100% contain;
    */
}

.pics-list .tips{
    text-align:left;
    color:#ff6000;
    padding-left:2%;
}
.btn-diaries-show{
    display:block;
    margin-top:10px;
}

.food-diaries .btnSprite{
    margin-top:12px;
}
.food-diaries .btnSprite .btn{
    width:80%;
    margin:0 auto;
    border-radius:5px;
    border:1px solid #a6a6a6;
    padding:15px;
    font-size:1rem;
    color:#ff5349;
    font-family:'Microsoft Yahei',arial;
    background:#ffffff url('../img/btn_share_1.png') 10% center no-repeat;
    background-size:contain;
    margin-bottom:12px;
}
.food-diaries .btnSprite .cancel{
    color:#cbcbcb;
    background:#ffffff url('../img/btn_share_2.png') 15% center no-repeat;
    background-size:contain;
}

.food-diaries .btnSprite .ui-state-hover{
    background-color:#ff8179;
    background-image:url('../img/btn_share_1-2.png');
    color:#ffffff;
    text-shadow: 0 0px 0 #F3F3F3;
}




#redirect{
    background-color:#ffffff;
}
.page-redirect{
    text-align:center;
    width:96%;
    /* margin:0 auto; */
    left:2%;
    overflow:hidden;
    font-family:"Microsoft Yahei",arial;

}
.page-redirect .header-top{
    padding-top:50%;
    background:url('../img/pic_404.png') left top no-repeat;
    background-size:contain;
}
.page-redirect .header-top h2,
.page-redirect .header-top h2 p{
    font-size:1.5rem;
    color:#5d614a;
    font-weight:normal;
    font-family:"Microsoft Yahei",arial;
    margin:0;
}

.page-redirect  .comment p{
    margin-top:55px;
    font-size:1rem;
    color:#5d614a;
    font-family:"Microsoft Yahei",arial;
}
.page-redirect  .btnSprite a{
    display:block;
    width:90%;
    height:55px;
    margin:0 auto;
    background:url('../img/btn_3.png') center top no-repeat;
    background-size:contain;
}





