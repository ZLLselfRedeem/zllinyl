using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace VAGastronomistMobileApp.Model
{
    /// <summary>
    /// 多语言基本信息
    /// </summary>
    public class LanguageInfo
    {
        /// <summary>
        /// 语言编号
        /// </summary>
        public int LangID { get; set; }
        /// <summary>
        /// 语言名称
        /// </summary>
        public int LangName { get; set; }
        /// <summary>
        /// 是否默认语言
        /// </summary>
        public bool IsDefaultLang { get; set; }
        /// <summary>
        /// 语言状态
        /// 0：已删除，1：正常
        /// </summary>
        public int LangStatus { get; set; }
    }
}
