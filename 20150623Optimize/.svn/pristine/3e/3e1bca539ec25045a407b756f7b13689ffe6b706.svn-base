.sum-layout{
	background-color:#ffffff;
	border:1px solid #000000;
	font-family:"Microsoft Yahei",arial;
	padding-bottom:22px;
}
.sum-layout .headerItem{
	font-size:16px;
	text-align:center;
	font-weight:normal;
}
.sum-layout .text label{
	display: block;
    float: left;
    text-align: right;
    width: 70px;
	position:relative;
	top:8px;
}
.sum-layout .text label .tips
{
    position:absolute;
    width:120px;
    left:25px;
    top:-2px;
    color:#ff0000;
    opacity:0;
    display:none;
}
.sum-layout .text .inputText,
.sum-layout .text .area{
	background-color: #FFFFFF;
    border: 1px solid #E3EAF0;
    font-size: 12px;
    height: 33px;
    line-height: 33px;
    margin-right: 8px;
    padding: 0 3px;
    resize: none;
    width: 204px;
}
.sum-layout .text .comment{
	font-size: 12px;
    line-height:18px;
	padding-left: 70px;
}
.sum-layout .text .area{
	height: 72px;
}

.sum-layout .text .btnSprite{
	margin-top: 12px;
    padding-left: 70px;
	height:27px;
}
.sum-layout .text .btnSprite .btn,
.sum-layout .text .btnSprite .btn:visited{
	float:left;
	color:#ffffff;
	text-align:center;
	background-position:left top;
	background-repeat:no-repeat;
	width:90px;
	height:27px;
	display:block;
	margin-right:8px;
	line-height:27px;
}
.sum-layout .text .btnSprite .comfirm{
	background-image:url(../Images/btn_1.png);
}
.sum-layout .text .btnSprite .cancel,
.sum-layout .text .btnSprite .cancel:visited{
	background-image:url(../Images/btn_2.png);
	color:#7f7f81;
}

