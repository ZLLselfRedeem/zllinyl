    display: block;
    padding-left: 10px;
    background: url(../images/ubox-select.gif) no-repeat 0px -24px;
    padding-bottom: 0px;
    width: 230px;
    color: #79a2bd;
    line-height: 24px;
    padding-top: 0px;
    height: 24px;
}
