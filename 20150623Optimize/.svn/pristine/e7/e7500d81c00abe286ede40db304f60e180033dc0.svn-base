        /// <summary>
        /// 开始日期
        /// </summary>
        public DateTime BeginDate { set; get; }

        /// <summary>
        /// 结束日期
        /// </summary>
        public DateTime EndDate { set; get; }

        /// <summary>
        /// 查询笔数
        /// </summary>
        public int QueryNumber { set; get; }

        /// <summary>
        /// 查询开始笔数
        /// </summary>
