CREATE TABLE [dbo].[AuthorityInfo] (
    [AuthorityID]          INT            IDENTITY (1, 1) NOT NULL,
    [AuthorityName]        NVARCHAR (50)  NULL,
    [AuthorityURL]         NVARCHAR (500) NULL,
    [AuthorityRank]        INT            NULL,
    [AuthorityDescription] NVARCHAR (500) NULL,
    [AuthoritySequence]    INT            NULL,
    [AuthorityStatus]      INT            NULL,
    [AuthorityType]        NVARCHAR (50)  NULL,
    CONSTRAINT [PK_Authority] PRIMARY KEY CLUSTERED ([AuthorityID] ASC)
);

