body,
form,
div,
textarea,
ul,
li,
img,
input {
  padding: 0;
  margin: 0;
}
body {
  font-family: arial, 'Microsoft Yahei', '微软雅黑';
  font-size: 14px;
  line-height: 1;
}
.main {
  max-width: 640px;
  width: 100%;
  margin-left: auto;
  margin-right: auto;
}
.main .header-top {
  color: #333;
  text-shadow: 0 1px 0 #f3f3f3;
  font-size: 15px;
  line-height: 20px;
  background: #f9f9f9;
  border-bottom: 1px solid #c7cdd3;
  padding: 20px 15px;
  overflow: hidden;
  background:url(../img/share_bg_header.png);
}
.main .header-top > div {

  max-width: 50%;
  white-space: nowrap;
  overflow: hidden;
}
.main .header-top > #shareDate {

}
.main .header-top > #shopName {

}
.main .form-area {
  padding: 6px 15px;
}
.main .form-area .area {
  width: 100%;
  font-size: 14px;
  min-height: 65px;
  border-width: 0;
  outline: none;
  resize: none;
  color: #cbcbcb;
}
.main .form-area .area :focus {
  -webkit-box-shadow: none;
  -moz-box-shadow: none;
  box-shadow: none;
}
.main .area-text {
  background: #fff;
  padding-bottom: 10px;
  padding-right: 15px;
  overflow: hidden;
}
.main .area-text .txt {
  margin-right: 5px;
  width: 90px;
  height: 24px;
  background: #f9f9f9;
  font-size: 14px;
  cursor: pointer;
  float: right;
  text-align: center;
  vertical-align: middle;
  line-height: 24px;
  margin-top: auto;
  margin-bottom: auto;
  -webkit-border-radius: 12px;
  -moz-border-radius: 12px;
  border-radius: 12px;
}
.main .area-text .txt :first-child {
  margin-right: 0;
}
.main .area-text .txt img {
  border: none;
  height: 12px;
  margin-right: 2px;
}
.main .area-text .clear-txt {
  color: #ff3c3c;
}
.main .area-text .switch-txt {
  color: #00c232;
}
.main .area-bottom {
  border-top: 1px solid #c7cdd3;
  background: #f9f9f9;
  min-height: 15px;
  border-bottom: 1px solid #c7cdd3;
}
.main .img-container {
  padding-top: 10px;
  overflow: hidden;
}
.main .img-container ul {
  height: auto;
  overflow: hidden;
  padding: 0 11px;
}
.main .img-container ul li {
  float: left;
  display: block;
  margin: 5px 1.25%;
  position: relative;
  list-style-type: none;
}
.main .img-container ul li img {
  -webkit-border-radius: 8px;
  -moz-border-radius: 8px;
  border-radius: 8px;
  width: 100%;
  border: 1px solid #c7cdd3;
}
.main .img-container ul li .dishName {
  position: absolute;
  width: 95%;
  bottom: 6px;
  display: block;
  color: #fff;
  text-align: center;
  vertical-align: bottom;
  overflow: hidden;
  z-index: 999;
}
.main .img-container ul li .dishName div {
  overflow: hidden;
  width: 10em;
  margin-left: auto;
  margin-right: auto;
  white-space: nowrap;
  -ms-text-overflow: ellipsis;
  -o-text-overflow: ellipsis;
  text-overflow: ellipsis;
}
.main .img-container ul li .ico-remove {
  position: absolute;
  top: 2.5%;
  right: 4.5%;
  display: block;
  color: #fff;
  background: #7d7d7d;
  width: 20px;
  height: 20px;
  line-height: 20px;
  text-align: center;
  -webkit-border-radius: 10px;
  -moz-border-radius: 10px;
  border-radius: 10px;
}
.main .img-container ul li .mask {
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  -webkit-border-radius: 8px;
  -moz-border-radius: 8px;
  border-radius: 8px;
  /*width: 100%;*/
  border: none;
}
.main .img-container .small li {
  width: 47.5%;
}
.main .img-container .big li {
  width: 97.5%;
}
.main .recover {
  border-top: 1px solid #c7cdd3;
  clear: both;
  margin-top: 10px;
  width: 100%;
  float: left;
  padding-top: 10px;
  text-align: center;
}
.main .recover span {
  display: inline-block;
  width: 100%;
  font-size: 14px;
  color: #959595;
}
.main .recover li {
  width: 40.625%;
  float: left;
  list-style-type: none;
  background: #e5e5e5;
  color: #7d7d7d;
  font-size: 15px;
  -webkit-border-radius: 4px;
  -moz-border-radius: 4px;
  border-radius: 4px;
  margin: 4.6875%;
  line-height: 32px;
  display: block;
}
.main .recover li div {
  overflow: hidden;
  margin-left: auto;
  margin-right: auto;
  white-space: nowrap;
  -ms-text-overflow: ellipsis;
  -o-text-overflow: ellipsis;
  text-overflow: ellipsis;
}
.main .toolbar {
  padding-right: 15px;
  overflow: hidden;
}
.main .toolbar label {
  float: right;
  margin-right: 10px;
}
.main .toolbar label input[type='checkbox'] {
  -webkit-appearance: none;
  -moz-appearance: none;
  appearance: none none;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  height: 14px;
  width: 14px;
  vertical-align: bottom;
  content: url('data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABoAAAAaBAMAAABbZFH9AAAAJ1BMVEVHsO3///9HsO1HsO1HsO1HsO1HsO1HsO1HsO1HsO1HsO1HsO1HsO3a+H6MAAAADHRSTlMAAAUGTU6Xzs/6+/6heOwKAAAAQUlEQVQY02MQqTwDA9MdGaLPIMBWhppkBhAQBAKz4wxzDOA8oVMMZxjgPMEzI4yHGhI9SXCe2nG0EBTtgnNWBAIAdPtPv/MY41kAAAAASUVORK5CYII=') /*../img/checkbox1.png*/;
  margin-right: 5px;
}
.main .toolbar label input[type='checkbox']:checked {
  content: url('data:image/png;base64,') /*../img/checkbox2.png*/;
}
.main .footer {
  width: 100%;
  min-height: 90px;
  padding: 15px;
}
.main .show-header-top {
  width: 100%;
  position: relative;
}
.main .show-header-top img {
  width: 100%;
}
.main .show-header-top > div {
  position: absolute;
  bottom: 2px;
  color: #fff;
}
.main .show-header-top #shareDate {
  left: 15px;
}
.main .show-header-top #shopName {
  right: 15px;
}
.main .note {
  text-indent: 2em;
  padding: 10px 2.34375%;
}
.main .double-line {
  border-top: 1px solid #c7cdd3;
  border-bottom: 1px solid #c7cdd3;
  padding-top: 5px;
  padding-bottom: 5px;
}
.main .double-line .tips {
  clear: both;
  float: left;
  color: #ff6000;
}
